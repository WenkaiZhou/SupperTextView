<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="custom_button_font_setting_style">
        <item name="android:layout_width">45dp</item>
        <item name="android:layout_height">36dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center</item>
        <item name="textNormalSolidColor">@color/color_ffffff</item>
        <item name="textPressedSolidColor">@color/color_464646</item>
        <item name="textNormalTextColor">@color/color_464646</item>
        <item name="textSelectedTextColor">@color/color_ffffff</item>
        <item name="textStrokeWidth">1dp</item>
        <item name="textStrokeColor">@color/color_464646</item>
        <item name="textIsSelected">true</item>
    </style>

</resources>
